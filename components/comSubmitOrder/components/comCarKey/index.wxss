/* components/comSubmitOrder/components/comCarKey/index.wxss */
.con-box{
  width:100%;
  padding:24rpx 20rpx 0rpx 20rpx;
  background: #F5F7FA;
  position: relative;
  box-sizing: border-box;
}
.btn-item-box{
  width:100%;
  display:flex;
  flex-direction: row;
  flex-wrap: nowrap;
  justify-content:space-between;
}
.bottom-box{
  display:flex;
  justify-content: space-between;
}
.confirm-btn{
width:192rpx;
height:104rpx;
background: #FF8833;
box-shadow: 0rpx 4rpx 4rpx 0rpx rgba(56, 70, 140, 0.1);
border-radius: 8rpx;
font-size: 36rpx;
font-family: PingFangSC-Regular, PingFang SC;
font-weight: 400;
color: #FFFFFF;
text-align:center;
line-height: 104rpx;
}
.pro-btn{
  width: 88rpx;
  height: 104rpx;
  line-height: 104rpx;
  text-align:center;
  background: #FFFFFF;
  box-shadow: 0rpx 4rpx 4rpx 0rpx rgba(56, 70, 140, 0.1);
  border-radius: 8rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #000000;
  display:inline-block;
  margin-right:16rpx;
  margin-bottom:24rpx;
}
.pro-btn:nth-of-type(7n)
{
  /* background:#ff0000; */
  margin-right:0rpx;
}
.active-pro-btn{
  background: #FF8833;
  box-shadow: 0rpx 4rpx 4rpx 0rpx rgba(56, 70, 140, 0.1);
  border-radius: 8rpx;
  color: #FFFFFF;
}