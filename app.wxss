/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.re-load{
  margin: 0 auto;
  margin-top:34rpx;
  width: 208rpx;
  height: 80rpx;
  box-shadow: 0rpx 8rpx 24rpx 0rpx rgba(219, 230, 255, 0.4);
  border-radius: 40rpx;
  border: 2rpx solid #FF8833;
  display:flex;
  justify-content: center;
  align-items:center;
  font-size: 28rpx;
  font-family: PingFangSC-Bold, PingFang SC;
  font-weight: bold;
  color: #FF8833;
  text-shadow: 0rpx 8rpx 24rpx rgba(219, 230, 255, 0.4);
}

::-webkit-scrollbar {
  display:none;
  width:0;
  height:0;
  color:transparent;
}
